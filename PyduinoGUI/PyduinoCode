    def comport_list(self):
        Com_list = []
        ports_list = list(serial.tools.list_ports.comports())
        for port in ports_list:
            Com_list.append(port[0])
            return Com_list
    
    def openPort(self):
        self.Pyduino = serial.Serial()
        self.Pyduino.port = self.cb_comport.currentText()
        self.Pyduino.baurdrate = 9600
        self.Pyduino.open()
        print("Port is Opened")
      
    def closePort(self):
        self.Pyduino.close()
        print("Port is Closed")

    def rxRead(self):
        rx_buffer = self.Pyduino.read()
        print(rx_buffer)


    def CH01ON(self):
        self.Pyduino.write(b'A')
    
    def CH01OFF(self):
        self.Pyduino.write(b'a')

    def CH02ON():
        pass

    def CH02OFF():
        pass


    def lcd_update(self):
        self.currentTime = QTime.currentTime()
        self.currentTimeText = self.currentTime.toString('hh:mm:ss')
        self.lcdDatetime.display(self.currentTimeText)

        global Hours,Mins,Sec
        currentTime = QTime.currentTime()
        Hours = currentTime.hour()
        Mins = currentTime.minute()
        Sec = currentTime.second()



