    def comport_list(self):
        Com_list = []
        ports_list = list(serial.tools.list_ports.comports())
        for port in ports_list:
            Com_list.append(port[0])
            return Com_list

    
    def openPort(self):
        #Pyduino
        self.Pyduino = serial.Serial()
        self.Pyduino.port = self.cb_comport.currentText()
        self.Pyduino.baurdrate = 9600
        self.Pyduino.open()
        print("Port is Opened")
    
    def checkBox_t1(self):
        if self.ckb_t1.isChecked():
            if Hours == valueSPN_Hon_t1:
                print("Now is " + str(Hours))
        else:
            pass
    
